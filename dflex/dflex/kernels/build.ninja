cflags = -DTORCH_EXTENSION_NAME=kernels -DTORCH_API_INCLUDE_EXTENSION_H -DPYBIND11_COMPILER_TYPE=\"_gcc\" -DPYBIND11_STDLIB=\"_libstdcpp\" -DPYBIND11_BUILD_ABI=\"_cxxabi1011\" -I/lustre/fswork/projects/rech/ugb/uzp81xx/DiffRL/dflex/dflex -isystem /gpfsdswork/projects/rech/ugb/uzp81xx/miniconda3/envs/shac/lib/python3.8/site-packages/torch/include -isystem /gpfsdswork/projects/rech/ugb/uzp81xx/miniconda3/envs/shac/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -isystem /gpfsdswork/projects/rech/ugb/uzp81xx/miniconda3/envs/shac/lib/python3.8/site-packages/torch/include/TH -isystem /gpfsdswork/projects/rech/ugb/uzp81xx/miniconda3/envs/shac/lib/python3.8/site-packages/torch/include/THC -isystem /gpfsdswork/projects/rech/ugb/uzp81xx/miniconda3/envs/shac/include/python3.8 -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++14 -Z -O2 -DNDEBUG
